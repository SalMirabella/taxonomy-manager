    {
        "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event",
        "@type": [
            "http://www.w3.org/2002/07/owl#Class"
        ],
        "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
            {
                "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"
            },
            {
                "@id": "_:node942"
            },
            {
